public class Snake {
		
		public Boolean m_isHead;
		public Boolean m_isTail;
		public int m_numSnake;
		
		Snake(){
			
		}
		public Boolean isHead(){
			return m_isHead;
		}
		public Boolean isTail(){
			return m_isTail;
		}
		public void setSnake(){
			
		}
		public int getSnake(){
			return m_numSnake;
		}
		
	}
	
